  analog function real get_nuintd;
    input nf, minsd;
    real  nf, minsd;
  
    begin
     if ((nf%2) != 0)
       get_nuintd = 2.0 * max((nf - 1.0) / 2.0, 0.0);
    else
      begin
        if (minsd == 1) 
          get_nuintd = 2.0 * max((nf / 2.0 - 1.0), 0.0);
        else
          get_nuintd = nf;
      end
    end
  endfunction
  
  analog function real get_nuendd;
    input nf, minsd;
    real  nf, minsd;
  
    begin
      if ((nf%2) != 0)
        get_nuendd = 1.0;
      else
        begin
          if (minsd == 1) 
            get_nuendd = 2.0;
          else
            get_nuendd = 0.0;
        end
    end
  endfunction
  
